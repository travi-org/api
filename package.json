{
  "name": "travi-api",
  "main": "index.js",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "private": true,
  "repository": "travi/travi-api",
  "scripts": {
    "lint:js": "eslint . --cache",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "nyc run-s tests:unit:base",
    "tests:unit:base": "mocha --recursive test/unit",
    "tests:integration:base": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}' --exit",
    "tests:integration": "run-s tests:integration:base -- --tags 'not @wip'",
    "tests:integration:wip": "run-s tests:integration:base -- --tags @wip",
    "tests:pact": "run-s pact:verify",
    "prepact:verify": "run-s start & echo $! > $TMPDIR/pact-verify.pid",
    "pact:verify": "babel-node scripts/verify-pacts.js",
    "postpact:verify": "kill -TERM `cat $TMPDIR/pact-verify.pid`",
    "test": "run-s lint:* tests:*",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "babel": "babel lib --source-maps --out-dir dist",
    "build": "run-p babel",
    "start": "node dist/app.js",
    "heroku:production": "run-s build",
    "postinstall": "grunt heroku:production",
    "dev": "nodemon lib/app.js",
    "precommit": "npm test",
    "commitmsg": "commitlint -e",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": "10.7.x",
    "npm": "6.1.x"
  },
  "devDependencies": {
    "@travi/any": "^1.7.5",
    "@travi/eslint-config-travi": "^1.6.18",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "4.1.2",
    "commitlint-config-travi": "^1.2.30",
    "coveralls": "^3.0.2",
    "cucumber": "4.2.1",
    "cz-conventional-changelog": "2.1.0",
    "dotenv": "^6.0.0",
    "formatio": "1.2.0",
    "globstar": "1.0.0",
    "grunt": "^1.0.3",
    "grunt-babel": "7.0.0",
    "grunt-cli": "1.2.0",
    "grunt-flyway": "travi/grunt-flyway#credentials",
    "hoek": "5.0.3",
    "husky": "1.0.0-rc.2",
    "load-grunt-config": "1.0.1",
    "markdownlint-cli": "^0.11.0",
    "mocha": "^5.2.0",
    "nodejs-dashboard": "0.4.3",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "oz": "^4.0.5",
    "pact": "4.3.2",
    "proxyquire": "2.0.1",
    "referee": "1.2.0",
    "referee-more-assertions": "0.2.0",
    "referee-sinon": "1.0.3",
    "setup-referee-sinon": "0.3.0",
    "sinon": "^4.5.0",
    "sinon-as-promised": "4.0.3",
    "time-grunt": "1.4.0"
  },
  "dependencies": {
    "bell": "^8.9.0",
    "boom": "7.2.0",
    "clone": "2.1.1",
    "deep-freeze": "0.0.1",
    "dotenv-safe": "^6.0.0",
    "glue": "^4.2.0",
    "good": "^7.3.0",
    "good-console": "7.1.0",
    "good-squeeze": "^5.1.0",
    "halacious": "5.0.1",
    "hapi": "^18.0.0",
    "hapi-auth-cookie": "^7.0.0",
    "hapi-graceful-shutdown-plugin": "^1.0.1",
    "hapi-mustache": "0.0.1",
    "hapi-swaggered": "^2.12.2",
    "hapi-swaggered-ui": "2.6.2",
    "http-status-codes": "1.3.0",
    "inert": "^4.2.1",
    "joi": "^13.4.0",
    "lodash": "4.17.10",
    "md5": "2.2.1",
    "newrelic": "^5.0.0",
    "path": "0.12.7",
    "query-string": "^6.1.0",
    "scarecrow": "^3.0.4",
    "snyk": "^1.89.0",
    "vision": "^4.1.1",
    "visionary": "6.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
