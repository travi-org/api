{
  "name": "travi-api",
  "main": "index.js",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "private": true,
  "repository": "travi/travi-api",
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "mocha --recursive test/unit",
    "tests:integration": "cucumber.js test/integration --compiler js:babel-register --tags 'not @wip'",
    "tests:integration:wip": "cucumber.js test/integration --compiler js:babel-register --tags @wip",
    "test": "run-s lint:* coverage tests:integration",
    "coverage": "nyc run-s tests:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "babel": "babel lib --out-dir dist",
    "build": "run-p babel",
    "start": "node dist/app.js",
    "heroku:production": "run-s build",
    "postinstall": "grunt heroku:production",
    "dev": "nodemon lib/dev.js",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": "7.3.x",
    "npm": "3.10.x"
  },
  "devDependencies": {
    "@travi/any": "1.1.0",
    "@travi/eslint-config-travi": "1.2.14",
    "babel-cli": "6.24.1",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.0.2",
    "coveralls": "2.13.1",
    "cucumber": "2.3.1",
    "cz-conventional-changelog": "2.0.0",
    "dotenv": "4.0.0",
    "dotenv-safe": "4.0.4",
    "formatio": "1.2.0",
    "globstar": "1.0.0",
    "grunt": "1.0.1",
    "grunt-babel": "6.0.0",
    "grunt-cli": "1.2.0",
    "grunt-flyway": "travi/grunt-flyway#credentials",
    "hoek": "4.1.1",
    "husky": "0.13.4",
    "load-grunt-config": "0.19.2",
    "markdownlint-cli": "0.3.0",
    "mocha": "3.4.2",
    "nodejs-dashboard": "0.4.1",
    "npm-run-all": "4.0.2",
    "nyc": "11.0.2",
    "oz": "4.0.5",
    "proxyquire": "1.8.0",
    "referee": "1.2.0",
    "referee-more-assertions": "0.2.0",
    "referee-sinon": "1.0.3",
    "setup-referee-sinon": "0.3.0",
    "sinon": "2.3.5",
    "sinon-as-promised": "4.0.3",
    "time-grunt": "1.4.0",
    "validate-commit-msg": "2.12.2"
  },
  "dependencies": {
    "bell": "8.7.0",
    "boom": "5.1.0",
    "clone": "2.1.1",
    "deep-freeze": "0.0.1",
    "glue": "4.1.0",
    "good": "7.2.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.2",
    "halacious": "5.0.0",
    "hapi": "16.4.3",
    "hapi-auth-cookie": "7.0.0",
    "hapi-mustache": "0.0.1",
    "hapi-swaggered": "2.9.0",
    "hapi-swaggered-ui": "2.6.0",
    "http-status-codes": "1.1.6",
    "inert": "4.2.0",
    "joi": "10.6.0",
    "lodash": "4.17.4",
    "md5": "2.2.1",
    "newrelic": "1.40.0",
    "path": "0.12.7",
    "query-string": "4.3.4",
    "scarecrow": "3.0.4",
    "snyk": "1.35.0",
    "vision": "4.1.1",
    "visionary": "6.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
