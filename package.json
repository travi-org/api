{
  "name": "travi-api",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/travi/travi-api"
  },
  "scripts": {
    "lint:js": "eslint .",
    "tests:unit": "mocha --recursive test/unit",
    "tests:integration": "cucumber.js test/integration --compiler js:babel-register --tags ~@wip --strict",
    "tests:integration:wip": "cucumber.js test/integration --compiler js:babel-register --tags @wip",
    "test": "run-s lint:* coverage tests:integration",
    "coverage": "nyc run-s tests:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "babel": "babel lib --out-dir dist",
    "build": "run-p babel",
    "start": "node dist/app.js",
    "postinstall": "grunt heroku:production",
    "dev": "nodemon lib/dev.js",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg"
  },
  "engines": {
    "node": "6.6.x",
    "npm": "3.10.x"
  },
  "devDependencies": {
    "@travi/any": "1.0.0",
    "@travi/eslint-config-travi": "0.3.0",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015-node": "6.1.1",
    "chai": "3.5.0",
    "coveralls": "2.11.15",
    "cucumber": "1.3.1",
    "cz-conventional-changelog": "1.2.0",
    "dotenv-safe": "3.0.0",
    "formatio": "1.2.0",
    "grunt": "1.0.1",
    "grunt-babel": "6.0.0",
    "grunt-cli": "1.2.0",
    "grunt-flyway": "travi/grunt-flyway#credentials",
    "hoek": "4.1.0",
    "husky": "0.12.0",
    "istanbul": "1.1.0-alpha.1",
    "load-grunt-config": "0.19.2",
    "mocha": "3.2.0",
    "npm-run-all": "3.1.2",
    "nyc": "10.0.0",
    "proxyquire": "1.7.10",
    "referee": "1.2.0",
    "referee-more-assertions": "0.2.0",
    "referee-sinon": "1.0.3",
    "setup-referee-sinon": "0.3.0",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2",
    "time-grunt": "1.4.0",
    "validate-commit-msg": "2.8.2"
  },
  "dependencies": {
    "bell": "8.4.0",
    "deep-freeze": "0.0.1",
    "glue": "4.0.0",
    "good": "7.1.0",
    "good-console": "6.2.0",
    "good-squeeze": "5.0.1",
    "halacious": "3.7.1",
    "hapi": "16.0.2",
    "hapi-auth-cookie": "6.1.1",
    "hapi-mustache": "0.0.1",
    "hapi-swaggered": "2.7.0",
    "hapi-swaggered-ui": "2.5.1",
    "http-status-codes": "1.0.6",
    "inert": "4.0.4",
    "joi": "10.0.6",
    "lodash": "4.17.2",
    "md5": "2.2.1",
    "newrelic": "1.36.0",
    "path": "0.12.7",
    "query-string": "4.2.3",
    "scarecrow": "3.0.4",
    "vision": "4.1.1",
    "visionary": "6.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
